package cubo;

/**
 *
 * @author omar
 */
public class Cuadrado {
    protected float l;
    
    protected float calcularArea(float l){
        float area = l * l;
        return area;
    }
    
    protected float calcularPerimetro(float l){
        float peri = l * 4;
        return peri;
    }
}
